{
  "name": "@elog/cli",
  "version": "0.2.1-beta.2",
  "description": "elog的脚手架，主要用来生成配置文件并调用@elog/core的API",
  "homepage": "https://github.com/LetTTGACO/elog/tree/master/packages/elog-cli#readme",
  "author": "1874 <me@mail.1874.cool>",
  "main": "dist/cjs/index.cjs.js",
  "module": "dist/esm/index.mjs.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./index.d.ts"
    }
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "elog": "bin/elog.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LetTTGACO/elog.git"
  },
  "bugs": {
    "url": "https://github.com/LetTTGACO/elog/issues"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "rollup -c"
  },
  "dependencies": {
    "@elog/core": "workspace:*",
    "@elog/shared": "workspace:*",
    "commander": "^9.4.1",
    "dotenv": "^16.0.3",
    "inquirer": "^9.1.4",
    "latest-version": "5.1.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.5",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.3.13"
  }
}
